{
  "name": "ngx-raven-wrapper",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --watch=false --single-run=true --browser=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:build": "cp README.md src/lib && ng-packagr -p src/lib/package.json",
    "lib:link": "npm run lib:build && cd dist/lib && npm link && cd ../../ && npm link ngx-raven",
    "lib:link:ci": "cd dist/lib && npm link && cd ../../ && npm link ngx-raven",
    "prelib:publish": "npm run lib:build",
    "lib:publish": "cd dist/lib && npm publish",
    "lib:build:publish": "npm run lib:build && cd dist/lib && npm publish",
    "lib:publish:ci": "cd dist/lib && ci-publish",
    "prelib:publish:ci": "cd dist/lib && npm version $CI_BUILD_TAG --allow-same-version && cd ../../"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "5.2.7",
    "@angular/compiler": "5.2.7",
    "@angular/core": "5.2.7",
    "@angular/http": "5.2.7",
    "@angular/platform-browser": "5.2.7",
    "@angular/platform-browser-dynamic": "5.2.7",
    "core-js": "^2.4.1",
    "raven-js": "^3.21.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.2",
    "@angular/compiler-cli": "5.2.7",
    "@angular/language-service": "5.2.7",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/raven-js": "^3.10.0",
    "codelyzer": "^4.0.1",
    "ng-packagr": "^2.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "2.6.2"
  }
}
